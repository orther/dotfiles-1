[include]
  path = ~/.gitconfig_private
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = always
[alias]
  st = "!echo use g"
  ci = commit
  ciz = commit --no-edit
  cim = commit -m
  co = checkout
  br = branch
  bra = branch -a
  cp = cherry-pick
  m = merge --no-ff --no-commit
  f = fetch --prune
  pre = !git fetch --prune --all && git rebase
  puo = push -u origin HEAD
  cleanbr = !git branch --no-color --merged | grep -v \" \\(master\\|develop\\|integration\\|current-feature\\|production\\|staging\\)$\" | grep -v `git rev-parse --abbrev-ref HEAD` | xargs -L1 git branch -d
  cleanbra = !git branch -r --no-color --merged | grep -v \"/\\(master\\|develop\\|integration\\|current-feature\\|production\\|staging\\)$\" | grep -v `git rev-parse --abbrev-ref HEAD` | sed \"s/\\// :/\" | xargs -L1 git push && git cleanbr
  cleanbradry = !git branch -r --no-color --merged | grep -v \"/\\(master\\|develop\\|integration\\|current-feature\\|production\\|staging\\)$\" | grep -v `git rev-parse --abbrev-ref HEAD` | sed \"s/\\// :/\" | xargs -L1 echo
  di = diff
  dc = diff --cached
  amend = commit --amend
  amendr = commit --amend --reuse-message=HEAD
  aa = add --all .
  head = !git l -1
  h = !git head
  r = !git l -20
  ra = !git r --all
  ff = merge --ff-only
  pullff = pull --ff-only
  noff = merge --no-ff
  l = log --graph --abbrev-commit --date=relative
  la = !git l --all
  div = divergence
  gn = goodness
  gnc = goodness --cached
  fa = fetch --prune --all
  poh = push origin HEAD
  puoh = push -u origin HEAD
  b = branch
  ds = diff --stat=160,120
  dh = diff HEAD^
  ru = remote update --prune
  pr = pull-request
  prd = pull-request -b develop
  rho = !git stash -u 2>&1 > /dev/null && git reset --hard HEAD@{u}
  mtv = mergetool --tool=mvim3
  ri = rebase -i
  fr = !git f && git rho
  branches = "!for branch in `git branch -r | grep -v HEAD`;do git show --format=\"%ai  %<(15)%ar%<(17)%C(red)%an%C(reset) %C(yellow)$branch%C(reset)\" $branch | head -n 1; done | sort -r"
  oldest-ancestor = !zsh -c 'diff --old-line-format= --new-line-format= <(git rev-list --first-parent \"${1:-master}\") <(git rev-list --first-parent \"${2:-HEAD}\") | head -1' -
  x-lead-time = !sh -c 'echo $(( ($(git show -s --format=%ct $1) - $(git show -s --format=%ct $(git oldest-ancestor $1^2))) / 60 / 60 ))' -

[format]
  pretty=format:%C(yellow)%h%Creset -%C(red)%d%Creset %s %Cgreen(%ar) %C(bold blue)<%an>%Creset
[merge]
  summary = false
  tool = p4merge
  conflictstyle = diff3
[mergetool]
  keepBackup = false
  keepTemporaries = false
  prompt = false
[mergetool "mvim3"]
  keepBackup = false
  keepTemporaries = false
  cmd = /usr/local/bin/mvim -v -f -d -c \"wincmd J\" \"$MERGED\" \"$LOCAL\" \"$BASE\" \"$REMOTE\"
[mergetool "p4merge"]
  cmd = /Applications/p4merge.app/Contents/Resources/launchp4merge "$PWD/$BASE" "$PWD/$REMOTE" "$PWD/$LOCAL" "$PWD/$MERGED"
  keepTemporaries = false
  keepBackup = false
  trustExitCode = false
[diff]
  tool = vimdiff
[difftool]
  prompt = false
[rerere]
  enabled = true
[push]
  default = tracking
[core]
  excludesfile = /Users/aaronjensen/.gitignore_global
  autocrlf = input
  pager = bash -c '. ~/.bash/aliases && diff-highlight | strip_diff_leading_symbols | less -r' -
[color "diff-highlight"]
 oldNormal = "red bold"
 oldHighlight = "red bold 52"
 newNormal = "green bold"
 newHighlight = "green bold 22"
[difftool "sourcetree"]
  cmd = opendiff \"$LOCAL\" \"$REMOTE\"
  path = 
[mergetool "sourcetree"]
  cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
  trustExitCode = true
[init]
  templatedir = ~/.git_template
[browser "ff"]
  cmd = open -a Firefox.app
[pull]
  rebase = preserve
[branch]
  autosetuprebase = always
[credential]
  helper = osxkeychain
[status]
  showUntrackedFiles = all
[rebase]
  autosquash = true
[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
